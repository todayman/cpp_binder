{
	  "name": "cpp_binder"
	, "description": "A C++ binding generator for D."
	, "copyright": "Copyright Â© 2014-2016, Paul O'Neil"
    , "license": "GPL-3.0"
	, "authors": ["Paul O'Neil"]
    , "targetType": "executable"
	, "dependencies": {
        "libdparse": "~>0.2.1"
	}
    , "preBuildCommands": [
        "mkdir -p build"
      , "cd build; cmake .. && make -j"
    ]
    , "lflags": [
        "-Lbuild"
    ]
    , "libs": [
        "cpp_binder"
      , "clangTooling"
      , "clangDriver"
      , "clangFrontend"
      , "clangParse"
      , "clangSema"
      , "clangEdit"
      , "clangAnalysis"
      , "clangAST"
      , "clangLex"
      , "clangSerialization"
      , "clangBasic"
      , "LLVMMCParser"
      , "LLVMMC"
      , "LLVMAsmPrinter"
      , "LLVMAsmParser"
      , "LLVMOption"
      , "LLVMBitReader"
      , "LLVMCore"
      , "LLVMSupport"
      , "boost_filesystem"
      , "boost_system"
      , "curses"
      , "stdc++"
      , "dl"
    ]
    , "subPackages": [
        "./tests"
    ]
}
