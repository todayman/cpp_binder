{
    "clang_args": [
          "-D__STDC_LIMIT_MACROS"
        , "-D__STDC_CONSTANT_MACROS"
        , "-fPIC"
    ],
    "binding_attributes": {
        "size_t": {
            "strategy": {
                  "name": "replace"
                , "d_decl": "size_t"
            }
          , "target_module": "."
        }
        , "std::runtime_error": {
            "strategy": {
                  "name": "replace"
                , "d_decl": "std.runtime_error"
            }
          , "target_module": "manual_types"
        }
        , "clang::PrintingPolicy": {
            "strategy": {
                  "name": "replace"
                , "d_decl": "clang.PrintingPolicy"
            }
          , "target_module": "manual_types"
        }
        , "clang::SourceManager": {
            "strategy": {
                  "name": "replace"
                , "d_decl": "clang.SourceManager"
            }
          , "target_module": "manual_types"
        }
        , "clang::QualType": {
            "strategy": {
                  "name": "replace"
                , "d_decl": "clang.QualType"
            }
          , "target_module": "manual_types"
        }
        , "clang::Decl": {
            "strategy": {
                  "name": "replace"
                , "d_decl": "clang.Decl"
            }
          , "target_module": "manual_types"
        }
        , "clang::ASTUnit": {
            "strategy": {
                "name": "replace"
              , "d_decl": "clang.ASTUnit"
            }
          , "target_module": "manual_types"
        }
        , "clang::AccessSpecifier": {
            "strategy": {
                "name": "replace"
              , "d_decl": "clang.AccessSpecifier"
            }
          , "target_module": "manual_types"
        }
        , "clang::LanguageLinkage": {
            "strategy": {
                "name": "replace"
              , "d_decl": "clang.LanguageLinkage"
            }
          , "target_module": "manual_types"
        }
        , "clang::Type": {
            "strategy": {
                "name": "replace"
              , "d_decl": "clang.Type"
            }
          , "target_module": "manual_types"
        }
        , "clang::SourceLocation": {
            "strategy": {
                "name": "replace"
              , "d_decl": "clang.SourceLocation"
            }
          , "target_module": "manual_types"
        }
        , "DeclVisitor": {
            "bound": 0
        }
        , "TypeVisitor": {
            "bound": 0
        }
        , "Type::Kind": {
            "visibility":"public"
        }
        , "isCXXRecord": {
            "bound": 0
        }
        , "ConstDeclarationVisitor": {
            "bound": 0
        }
    }
}
