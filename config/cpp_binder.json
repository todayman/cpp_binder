{
    "clang_args": [
          "-D__STDC_LIMIT_MACROS"
        , "-D__STDC_CONSTANT_MACROS"
        , "-fPIC"
    ],
    "binding_attributes": {
        "clang::PrintingPolicy": {
            "strategy": {
                  "name": "replace"
                , "d_decl": "clang.PrintingPolicy"
            }
        }
        , "clang::SourceManager": {
            "strategy": {
                  "name": "replace"
                , "d_decl": "clang.SourceManager"
            }
        }
        , "string": {
            "strategy": {
                  "name": "replace"
                , "d_decl": "cpp_string"
            }
        }
    }
}
