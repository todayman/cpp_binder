            }
            , "target_module": "."
        },
        "signed char": {
            "strategy": {
                "name": "replace",
                "d_decl": "char"
            }
            , "target_module": "."
        },
        "unsigned char": {
            "strategy": {
                "name": "replace",
                "d_decl": "ubyte"
